package com.testng;

import org.testng.annotations.AfterMethod;
import org.testng.annotations.BeforeMethod;
import org.testng.annotations.Test;

public class SinpleAnnotation{
	@BeforeMethod
	private void url_launched() {
		System.out.println("url launched");
	}
	@Test
	private void dresses_Clicked(){
		System.out.println("Dresses Clicked");
	}
	@Test
	private void cassual_Dress() {
		System.out.println("cassual Dress Clicked");
	}
	@Test
	private void moveToElementCart() {
		System.out.println("moveToElementCart and addToCart Clicked");
	}
	@AfterMethod
	private void url_Closed(){
		System.out.println("url closed");
	}
	
	
}