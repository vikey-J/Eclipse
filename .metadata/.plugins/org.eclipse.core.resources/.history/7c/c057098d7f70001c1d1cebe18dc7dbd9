package com.string;

public class String_Function {
	public static void main(String[] args) {
		String s="VIKNESHKUMAR";
		int i = s.length();
		System.out.println("length :"+i);
		
		boolean b = s.equals("vikneshkumar");
		System.out.println("given value is equal ? :"+b);
		
		boolean case1 = s.equalsIgnoreCase("vikneShKUMar");
		System.out.println("this is equal : "+case1);
		
		String upperCase = s.toUpperCase();
		System.out.println("upper :"+upperCase);
		
		String lowerCase = s.toLowerCase();
		System.out.println("lower :" + lowerCase);
		
		boolean startsWith = s.startsWith("V");
		System.out.println("start with v :"+startsWith);
		
		boolean endsWith = s.endsWith("R");
		System.out.println("endswith R :"+endsWith);
		
		boolean contains = s.contains(lowerCase);
		System.out.println("contains :"+contains);
		
	}

}
