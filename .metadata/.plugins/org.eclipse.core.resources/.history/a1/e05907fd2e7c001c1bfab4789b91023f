package com.interviewPrograms;

public class Sample {
	public static void main(String[] args) {
		String s="VIKEY";
		String rs="";
		char ch;
		System.out.println("original word : "+s);
        for (int i = 0; i < s.length(); i++) {
        	ch=s.charAt(i);
        	rs=ch+rs;
        	System.out.println(ch);
		}	
//        System.out.println("reverseword : "+rs);
       
		

	}

}
