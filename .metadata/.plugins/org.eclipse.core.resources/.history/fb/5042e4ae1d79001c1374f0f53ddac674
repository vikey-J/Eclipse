package com.interviewPrograms;

public class Abc {
	public static void main(String[] args) {
		int t = 154;
		int a = t, b, c = 0;
		while (a > 0) {
			b = a % 10;
			c = (b*b*b) + c;
			a = a / 10;
		}
		System.out.println(c);
		if (t == c) {
			System.out.println("palindrom");
		} else {
			System.out.println("not polindrome");
		}

		// fibanocii series
//		int a=0,b=1,c;
//		for (int i = 0; i < 10; i++) {
//			c=a+b;
//			a=b;
//			b=c;
//			System.out.println(c);
//		}

		// Amstrong number

	}

}